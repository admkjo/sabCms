ul.menu {
    margin: 0;
    padding: 0;
    line-height: 12px;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    -webkit-box-shadow: 0 2px 2px rgba(0, 0, 0, 0.25);
    -moz-box-shadow: 0 2px 2px rgba(0, 0, 0, 0.25);
    box-shadow: 0 2px 2px rgba(0, 0, 0, 0.25);
    -pie-box-shadow: 0 2px 0px rgba(0, 0, 0, 0.15);
}
.menu_horizontal ul.menu {
    display: block;
    /*width: 100%;*/
    width: 990px;
}
ul.menu li {
    margin: 0;
    padding: 0;
    position: relative;
    list-style: none outside none;
}
.menu_horizontal ul.menu li {
    float: left;
}
.menu_horizontal ul.menu li ul li {
    float: none;
}
/* main level link */
ul.menu a,
ul.menu .separator {
    font-family: Verdana, Helvetica, sans-serif !important;
    font-size: 1.1em;
    text-decoration: none;
    display: block; 
    padding: 14px 20px;
    margin: 0;
    text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.1);
}
.menu_horizontal ul.menu li a,
.menu_horizontal ul.menu li .separator {
    border-left: 1px solid rgba(0,0,0,0.1);
    border-right: 1px solid rgba(255,255,255,0.2);
}

/* Set border radius and border for the first menu item */
.menu_horizontal > ul.menu > li:first-child > a,
.menu_horizontal > ul.menu > li:first-child > .separator
{
    border-left: medium none;
    -moz-border-radius: 5px 0px 0px 5px;
    -webkit-border-radius: 5px 0px 0px 5px;
    border-radius: 5px 0px 0px 5px;
}

/* Set border radius and border for the last menu item */
ul.menu li:last-child a,
ul.menu li:last-child .separator {
    border-right: medium none;
    margin: 0;
}
.menu_horizontal > ul.menu > li:last-child > a,
.menu_horizontal > ul.menu > li:last-child > .separator
{
    /*Im coding*/
}

ul.menu li:last-child {
    border-bottom: medium none !important;
}
/*------*/
ul.menu ul li a,
ul.menu ul li .separator
{
    border-bottom: 1px solid rgba(0, 0, 0, 0.2);
    border-left: medium none !important;
    border-right: medium none !important;
    border-top: 1px solid rgba(255, 255, 255, 0.2);
    text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.1);
}
ul.menu ul li:hover a,
ul.menu ul li:hover .separator,
ul.menu ul li.active a,
ul.menu ul li.active .separator,
ul.menu ul li.current a,
ul.menu ul li.current .separator
{
    border-color: rgba(255, 255, 255, 0.2) -moz-use-text-color rgba(0, 0, 0, 0.5);
    border-left: medium none;
    border-right: medium none;
    border-style: solid none;
    border-width: 1px medium;
}
/* level 2 list */
ul.menu ul {
    display: inline-block;
    visibility:hidden;
    opacity:0;
    transition: visibility 0s linear 0.05s,opacity 0.3s linear 0s;
    -moz-transition: visibility 0s linear 0.05s,opacity 0.3s linear 0s;
    -o-transition: visibility 0s linear 0.05s,opacity 0.3s linear 0s;
    -ms-transition: visibility 0s linear 0.05s,opacity 0.3s linear 0s;
    padding: 0;
    min-width: 200px;
    white-space: nowrap;
    position: absolute;
    /*-webkit-border-radius: 5px;*/
    /*-moz-border-radius: 5px;*/
    /*border-radius: 5px;*/
    -webkit-box-shadow: 0 1px 3px rgba(0, 0, 0, .3);
    -moz-box-shadow: 0 1px 3px rgba(0, 0, 0, .3);
    box-shadow: 0 1px 3px rgba(0, 0, 0, .3);
    -pie-box-shadow: 0 2px 0px rgba(0, 0, 0, 0.15);
}

ul.menu,
ul.menu ul,
ul.menu ul ul {
    background: #333333;
    background: -moz-linear-gradient(top, #666 40%, #000 100%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(40%, #666), color-stop(100%, #000));
    background: -webkit-linear-gradient(top, #666 40%, #000 100%);
    background: -o-linear-gradient(top, #666 40%, #000 100%);
    background: -ms-linear-gradient(top, #666 40%, #000 100%);
    background: linear-gradient(top, #666 40%, #000 100%);
}
ul.menu li a,
ul.menu li span {
    color: #ffee00;
    text-shadow: 0px 0px 0px rgba(0,0,0,0.5);
}

ul.menu ul li a,
ul.menu ul li span {
    color: #ffffff !important;
    text-shadow: none;
}
ul.menu li:hover > a,
ul.menu li:hover > span,
ul.menu li.current > a,
ul.menu li.current > span,
ul.menu li.active > a,
ul.menu li.active > span,
ul.menu li ul li.current > a,
ul.menu li ul li.current > span,
ul.menu li ul li.active > a,
ul.menu li ul li.active > span,
ul.menu li ul li ul li.current > a,
ul.menu li ul li ul li.current > span,
ul.menu li ul li ul li.active > a,
ul.menu li ul li ul li.active > span {

    background: #888;
    background: -moz-linear-gradient(top, #aaa 40%, #222 100%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #aaa), color-stop(100%, #222));
    background: -webkit-linear-gradient(top, #aaa 0%, #222 100%);
    background: -o-linear-gradient(top, #aaa 0%, #222 100%);
    background: -ms-linear-gradient(top, #aaa 0%, #222 100%);
    background: linear-gradient(top, #aaa 0%, #222 100%);

    text-shadow: 0px -1px 0px rgba(0,0,0,0.3)!important;
}

ul.menu li ul li:hover > a,
ul.menu li ul li:hover > span,
ul.menu li ul li ul li:hover > a,
ul.menu li ul li ul li:hover > span{
    background: #999;
}

ul.menu li:hover > a,
ul.menu li:hover > span,
ul.menu li.current > a,
ul.menu li.current > span,
ul.menu li.active > a,
ul.menu li.active > span {
    
    text-shadow: 0px -1px 0px rgba(0,0,0,0.3);
}

ul.menu ul li:hover > a,
ul.menu ul li:hover > span,
ul.menu ul li.current,
ul.menu ul li.active > a,
ul.menu ul li.active > span {
    transition: all 0.3s ease-out 0s; 
    text-shadow: 0px -1px 0px rgba(0,0,0,0.8)!important;
}
ul.menu ul li:hover,
ul.menu ul li.current,
ul.menu ul li.active {
    border: none; 
} 

/* dropdown */
.menu_horizontal ul.menu li:hover > ul {
    visibility:visible;
    opacity:1;
    z-index: 900;
}
.menu_horizontal ul.menu li:hover > ul {
    visibility:visible;
    opacity:1;
}
ul.menu li ul li a,
ul.menu li ul li .separator
{
    width: auto;
    display: block;
    margin: 0;
    display: block;
    text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.1);
}
ul.menu li ul li:first-child a,
ul.menu li ul li:first-child .separator,
ul.menu li ul li ul li:first-child a,
ul.menu li ul li ul li:first-child .separator
{
    border-top: none;
    display: block;
}
ul.menu li ul li:last-child a,
ul.menu li ul li:last-child .separator {
    border-bottom: none;
}
ul.menu li ul li:only-child a,
ul.menu li ul li:only-child .separator {
    border: none;
}
.menu_horizontal ul.menu ul {
    margin: 0;
    top: 100%;
    left: 0%;
}
.menu_horizontal ul.menu ul li ul {
    margin: 0;
    top: 0%;
    left: 100%;
}

ul.menu ul li {
    float: none;
    margin: 0;
    padding: 0;
}

ul.menu ul a, ul.menu ul .separator {
    font-size: 1.0em;
    padding: 7px 20px; 
}
/* level 3+ list */
ul.menu ul ul {
    left: 100%;
    top: 0;
}
/* clearfix */
ul.menu:after {
    content: "";
    display: block;
    clear: both;
    line-height: 0;
    height: 0;
}
ul.menu {
    display: inline-block;
}
* html ul.menu {
    height: 1%;
} 



