package com.sabonay.cms.web.wrapper;

/**
 *
 * @author emma
 */
public class UserTask {
    private String index;
    private String page;
    private String title;
    
    public UserTask(String index, String page, String title){
        this.index = index;
        this.page = page;
        this.title = title;
    }

    public String getIndex() {
        return index;
    }

    public void setIndex(String index) {
        this.index = index;
    }

    public String getPage() {
        return page;
    }

    public void setPage(String page) {
        this.page = page;
    }

    public String getTitle() {
        return title;
    }

    public void setTitle(String title) {
        this.title = title;
    }
}
